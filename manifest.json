{
  "manifest_version": 2,

  "name": "Highlighter",
  "description": "Highlight any thing with this",
  "version": "1.0",
	
	"offline_enabled":true;
		
	"permissions":[
		"contextMenus",
		"tabs"
	],

    "content_scripts": [
    {
        "matches":["<all_urls>"],
        "js": ["inject.js"]
        }
     ]
}
